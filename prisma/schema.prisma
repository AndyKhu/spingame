// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique @default(cuid())
  username String @unique
  password String
}

model LuckySpiner {
  id String @id @unique @default(cuid())
  memberId String
  codeVoucher String
  canExpired Boolean
  expiredDate DateTime?
  expired Boolean
}

model Misteribox {
  id String @id @unique @default(cuid())
  memberId String
  codeVoucher String
  canExpired Boolean
  expiredDate DateTime?
  expired Boolean
}